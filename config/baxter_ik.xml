<?xml version="1.0" ?>
<ExoticaBaxterConfig>
  <IKsolver Name="MySolver">
    <MaxIt>100</MaxIt>
    <MaxStep>0.1</MaxStep>
    <Tolerance>1e-5</Tolerance>
    <Alpha>1.0</Alpha>
    <C>1e-3</C>
  </IKsolver>

  <UnconstrainedEndPoseProblem Name="IKProblem">
    <PlanningScene>
      <Scene Name="IKScene">
        <JointGroup>left_arm</JointGroup>
        <!--<URDF>{baxter_description}/urdf/baxter.urdf</URDF>-->
        <URDF>{exotica_baxter}/config/baxter_no_gazebo.urdf</URDF>
        <SRDF>{exotica_baxter}/config/baxter.srdf</SRDF>
        <!-- <AlwaysUpdateCollisionScene>1</AlwaysUpdateCollisionScene> -->
        <!-- <CollisionScene>CollisionSceneFCLLatest</CollisionScene> -->
        <Debug>0</Debug>
      </Scene>
    </PlanningScene>
  </UnconstrainedEndPoseProblem>
</ExoticaBaxterConfig>
